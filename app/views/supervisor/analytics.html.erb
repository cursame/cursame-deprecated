<div class="container">
  <div class="row">
    <div class="span16">
      <ul class="breadcrumb">
        <li><%= link_to(t('.admin'), supervisor_dashboard_path) %></li>
      </ul>  
    </div>
  </div>
  <div class="dashboard">  
    <div class="row">
      <div class="span5">
        <%= render :partial => "supervisor/shared/admin_menu" %>
      </div>    
      <div class="span-two-thirds">
        <div class="dashboard-box">
          <div class="underline padding-top">
            <h2>Analytics</h2>
          </div>
          <div id="notifications">
            <div class='notification'>
              <div class="date-published">
                <div class="date-published">10 al 17 de enero</div>
                <%= link_to 'Reporte de Usuarios', 'http://50.116.21.144/users-20130110-20130117.csv' %>
              </div>
            </div>
            <div class='notification'>
              <div class="date-published">
                <%= link_to 'Reporte de Posts', 'http://50.116.21.144/posts-20130110-20130117.csv' %>
              </div>
            </div>
            <div class='notification'>
              <div class="date-published">
                <%= link_to 'Reporte de Logins', 'http://50.116.21.144/logins.csv-20130110-20130117.csv' %>
              </div>
            </div>
            <div class='notification'>
              <div class="date-published">
                <%= link_to 'Reporte de Dispositivos', :controller=>"analytics", :action=>"devices", :format=>"csv" %>
              </div>
            </div>
            <div class='notification'>
              <div class="date-published">
                <div class="date-published">Diciembre</div>
                <%= link_to 'Reporte de Usuarios', 'http://50.116.21.144/users.csv' %>
              </div>
            </div>
            <div class='notification'>
              <div class="date-published">
                <%= link_to 'Reporte de Posts', 'http://50.116.21.144/posts.csv' %>
              </div>
            </div>
            <div class='notification'>
              <div class="date-published">
                <%= link_to 'Reporte de Logins', 'http://50.116.21.144/logins.csv' %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%= render :partial => "shared/footer" %>
